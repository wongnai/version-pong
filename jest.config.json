{
    "transform": {
        "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "((\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
        "ts",
        "tsx",
        "js",
        "jsx",
        "json",
        "node"
    ],
    "moduleDirectories": ["node_modules", "src"],
    "collectCoverageFrom": [
        "src/**/*.{js,jsx,ts,tsx}"
    ]
}
